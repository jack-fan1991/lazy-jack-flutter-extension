{
  "name": "lazy-jack",
  "displayName": "lazyJ tools",
  "description": "lets go",
  "version": "0.0.206",
  "publisher": "jackFan",
  "icon": "image/lazy_man.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "flutter",
    "bloc",
    "freezed"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "Lazy Jack Flutter Extension",
      "properties": {
        "lazy-jack-flutter-extension.customCubit": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Custom Cubit types for the Quick Fix."
        },
        "lazy-jack-flutter-extension.resultWrapper": {
          "type": "object",
          "default": {},
          "properties": {
            "name": {
              "type": "string",
              "description": "The name of the wrapper class, e.g., 'Result'."
            },
            "import": {
              "type": "string",
              "description": "The import statement for the wrapper class."
            }
          },
          "description": "Define a wrapper type (e.g., Result, Either) for feature data layer return values."
        },
        "lazy-jack-flutter-extension.dataReturnWrapper": {
          "type": "object",
          "default": {},
          "properties": {
            "name": {
              "type": "string",
              "description": "The name of the wrapper class, e.g., 'Result'."
            },
            "import": {
              "type": "string",
              "description": "The import statement for the wrapper class."
            }
          },
          "description": "Define a wrapper type (e.g., Result, Either) for feature data layer return values.",
          "deprecationMessage": "已改用 lazy-jack-flutter-extension.resultWrapper。"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lazy-sidebar",
          "title": "lazy-sidebar",
          "icon": "/image/rocket.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "fast-repo-gui",
        "contents": "Open github Repository \n[Open](command:command_open_github_repo)\n[Actions](command:command_open_github_actions)\n[Wiki](command:command_open_github_wiki)\nOpen local sourcetree\n[Open](command:command_open_sourcetree_local_repo)"
      }
    ],
    "views": {
      "lazy-sidebar": [
        {
          "id": "fast-repo-gui",
          "name": "fast-repo-gui"
        },
        {
          "id": "GitDataProvider",
          "name": "Git"
        },
        {
          "id": "FlutterDataProvider",
          "name": "Flutter"
        },
        {
          "id": "TypescriptDataProvider",
          "name": "Typescript"
        },
        {
          "id": "VscodeDataProvider",
          "name": "VS Code"
        }
      ]
    },
    "commands": [
      {
        "category": "lazy Jack",
        "command": "command_create_freezed_class_by_base_fileName",
        "title": "@freezed Create freezed class by file name"
      },
      {
        "category": "lazy Jack",
        "command": "command_create_freezed_union_class_by_base_fileName",
        "title": "@freezed Create freezed Union"
      },
      {
        "category": "lazy Jack",
        "command": "command_add_freezed_state_by_base_fileName",
        "title": "@freezed Add new freezed State by file name"
      },
      {
        "category": "lazy Jack",
        "command": "command_dart_selected_to_factory",
        "title": "To factory constructor (Selected field)"
      },
      {
        "category": "lazy Jack",
        "command": "command_dart_assert",
        "title": "✨ Dart Asset Transformer"
      },
      {
        "category": "lazy Jack",
        "command": "extension.updateDependencyVersion",
        "title": "Update Dependency Version"
      },
      {
        "category": "lazy Jack",
        "command": "command_dart_2_require_param",
        "title": "To require param (Selected field)"
      },
      {
        "category": "lazy Jack",
        "command": "command_clean_architecture",
        "title": "✨ Create Feature"
      },
      {
        "category": "lazy Jack",
        "command": "command_generate_flutter_page",
        "title": "✨ Create TypeSafe Page"
      },
      {
        "category": "lazy Jack",
        "command": "graphql_code_gen_fragment_to_dart_type_def",
        "title": "✨ graphql to dart type def"
      },
      {
        "category": "lazy Jack",
        "command": "lazyJack.generateCleanArchitecturePage",
        "title": "✨ Add sub page"
      },
      {
        "category": "lazy Jack",
        "command": "lazyJack.createCubit",
        "title": "✨ Add sub view"
      },
      {
        "category": "lazy Jack",
        "command": "lazyJack.addDataSourceMethod",
        "title": "✨ Add Data Source Method"
      },
      {
        "category": "lazy Jack",
        "command": "lazyJack.createModuleTemplate",
        "title": "✨ Create Module Template"
      },
      {
        "category": "lazy Jack",
        "command": "lazyJack.addModuleMethod",
        "title": "✨ Add Module Data Source Method"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_create_freezed_class_by_base_fileName",
          "group": "0_b_jack_dart_freezed@0"
        },
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_create_freezed_union_class_by_base_fileName",
          "group": "0_b_jack_dart_freezed@1"
        },
        {
          "when": "editorFocus && resourceLangId == dart && editorHasSelection",
          "command": "command_dart_2_require_param",
          "group": "0_A_jack_dart_selected@1"
        }
      ],
      "explorer/context": [
        {
          "command": "command_dart_assert",
          "title": "✨ Dart Asset Transformer",
          "when": "explorerResourceIsFolder  && (resourceFilename =~ /.*(svg|image).*/)"
        },
        {
          "command": "command_clean_architecture",
          "title": "✨ Create Clean Arch Feature Module",
          "when": "explorerResourceIsFolder && (resourceFilename == 'features' || resourcePath =~ /.*\\/presentation\\/pages$/ || resourcePath =~ /.*\\/features\\/shared$/)"
        },
        {
          "command": "lazyJack.createCubit",
          "title": "✨ Create Clean Arch Cubit",
          "when": "explorerResourceIsFolder && (resourceFilename =~ /.*(presentation).*/)"
        },
        {
          "command": "command_generate_flutter_page",
          "title": "✨Create TypeSafe Page",
          "when": "explorerResourceIsFolder && resourcePath =~ /.*lib.*/ && !(resourceFilename =~ /^(modules|features|presentation|models|bloc|domain|repository|usecases|di|data|sources|repo_impls|entities)$/)"
        },
        {
          "command": "graphql_code_gen_fragment_to_dart_type_def",
          "title": "✨ Graphql Fragment to dart typedef",
          "when": "explorerResourceIsFolder && resourceFilename == '__generated'"
        },
        {
          "command": "lazyJack.generateCleanArchitecturePage",
          "when": "explorerResourceIsFolder && resourcePath =~ /.*\\/features\\/.*\\/presentation$/",
          "title": "✨ Add Clean Arch Subpage module"
        },
        {
          "command": "lazyJack.addDataSourceMethod",
          "when": "explorerResourceIsFolder && resourcePath =~ /.*\\/features\\/.*\\/data$/"
        },
        {
          "command": "lazyJack.createModuleTemplate",
          "title": "✨ Create Module Template",
          "when": "explorerResourceIsFolder && resourceFilename == 'modules'"
        },
        {
          "command": "lazyJack.addModuleMethod",
          "title": "✨ Add Module Method",
          "when": "explorerResourceIsFolder && resourcePath =~ /.*[\\\\/]lib[\\\\/]modules[\\\\/][^\\\\/]+$/"
        }
      ]
    },
    "snippets": [
      {
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "dart",
        "path": "./snippets/dart_freezed.json"
      },
      {
        "language": "dart",
        "path": "./snippets/dart_unit_test.json"
      },
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "exclude": [
    "node_modules",
    ".vscode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jack-fan1991/lazy-jack-flutter-extension"
  },
  "extensionPack": [
    "jackFan.lazy-jack-flutter-freezed",
    "jackFan.lazy-jack-flutter-logger-easy-life",
    "jackFan.flutter-l10n-detector"
  ],
  "dependencies": {
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "axios": "^1.3.2",
    "change-case": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "iconv-lite": "^0.6.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.19",
    "@vscode/vsce": "^2.23.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.194"
  }
}