{
  "import freezed part": {
    "prefix": ["fz.p"],
    "body": ["part '${TM_FILENAME_BASE}.${1|g,freezed|}.dart';", "$2"],
    "description": "create part for freezed"
  },
  "Create Freezed Data Class": {
    "prefix": "fzClass",
    "body": [
      "@freezed",
      "abstract class ${1:DataClass} with _$${1}{",
      "  const ${1}._();",
      "  const factory ${1}(${2}) = _${1};",
      "}"
    ],
    "description": "Freezed Data Class"
  },
  "Create Freezed class with state": {
    "prefix": "fzWithState",
    "body": [
      "@freezed",
      "abstract class ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/g}} with _$$1{",
      "  const ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/g}}._();",
      "  const factory $1.${2:init}(${3}) = _${2/(.*)/${1:/capitalize}/};",
      "\t$4",
      "}"
    ],
    "description": "Create Freezed class with state class by base file name"
  },
  "Add New freezed State": {
    "prefix": "fzState",
    "body": [
      "const factory ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/g}}.${2:newState}(${3}) = _${2/(.*)/${1:/capitalize}/};",
      "\t$4"
    ],
    "description": "Add New Freezed State"
  },
  "Copy Class Name to Freezed State": {
    "prefix": "fzC2State",
    "body": [
      "const factory $CLIPBOARD.${2:newState}(${3}) = _${2/(.*)/${1:/capitalize}/};"
    ],
    "description": "Clipboard Class Name to Freezed State"
  }
  
}
